function handleOnclick(){
    const headingThree = document.getElementById('heading-three');
    headingThree.innerText = "Heading three is changed by button clicked";
    
}

// Option Two
document.getElementById('event-listener').addEventListener('click', function () {
    const headingStatus = document.getElementById('heading-three');
    headingStatus.innerText = "Heading changed By clicking on the Event Listener Button"
})